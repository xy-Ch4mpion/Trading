        //
        var loginName = $('#LeftTraderKeywords').val();
        // 是否选择了模糊搜索中的数据
        var isClickSearchTips = false;
        function _rankSearchLoop(_box) {
            return function () {
                rankSearchLoop(_box);
            }
        }
        var rankSearchLoop = function (box) {
            //var box = $('.rankSearch, .szRankSearch, .personRankSearch');
            if ($.trim(box.val()).length &gt;= 1) {
                $.post('/Handlers/Default/Match/AdminMatchQueryHandler.ashx/?method=GetRankSearchData',
                {
                    Keywords: box.val(),
                    rankType: box.attr('rankType'),
                    groupID:$("#dplGroup").val()
                },
                function (data) {
                    //复原标记位
                    isClickSearchTips = false;
                    //不存在DOM则新建
                    //if ($('#nameCodeInfos').length == 0) {
                        $('#nameCodeInfos').remove();
                        $(document.body).append('&lt;div id="nameCodeInfos" style="position: absolute; z-index: 999999999; text-align: left;background-color: #fff; border: 1px #ccc solid; display: none; _width:250px; overflow:hidden;"&gt;&lt;/div&gt;');
                        var infoLeft = box.offset().left;
                        var infoTop = box.offset().top + box.height();
                    //}
                    if (data.length &gt; 0) {
                        $('#nameCodeInfos').empty();
                        // 只显示前10条
                        for (var i = 0; i &lt; data.length &amp;&amp; i &lt; 10; i++) {
                            if (Object.prototype.toString.call(data[i]).slice(8, -1) == "Object")
                                $('#nameCodeInfos').append('&lt;a type="' + data[i].Type + '"&gt;' + data[i].Value + '&lt;/a&gt;');
                            else
                                $('#nameCodeInfos').append('&lt;a&gt;' + data[i] + '&lt;/a&gt;');
                        }

                        //if ($('iframe', '#nameCodeInfos').length == 0) {
                        //    $('#nameCodeInfos').append('&lt;iframe style="_width:250px;_height:200px; top:0; left:0;position: absolute; z-index: -1; background-color:#fff;" frameborder="0"&gt;&lt;/iframe&gt;');
                        //}
                        $('#nameCodeInfos').offset({
                            left: infoLeft,
                            top: infoTop
                        });
                        //显示搜索面板
                        $('#nameCodeInfos').show();
                        //触发文本框事件
                        $('.rankSearch, .szRankSearch, .personRankSearch').unbind('blur');
                        $('.rankSearch, .szRankSearch, .personRankSearch').blur(function () {
                            if ($('#nameCodeInfos') != undefined) {
                                $('#nameCodeInfos').hide();
                            }
                        });
                        $('#nameCodeInfos a').click(function () {
                            var tempVal = box.val();
                            if (box.val)
                                box.val($(this).text().split('/')[0]);
                            if (typeof ($(this).attr("type")) != "undefined") {//个人排行榜 会有这个属性值
                                if ($("#searchType"))//可查人、班级、赛组
                                    $("#searchType").val($(this).attr("type"));
                            }
                            $('#nameCodeInfos').hide();
                            if (box.val() != tempVal) {
                                box.change();
                            }
                            isClickSearchTips = true;
                            ////box.change();   //自动提示框选择前会导致下单的change事件触发，因此主动在填充后调用一次。
                        });
                    } else {
                        $('#nameCodeInfos').hide();
                    }
                },
                'json');
            } else {
                $('#nameCodeInfos').hide();
            }
        };
        var rankSearchImgUrl = '';
        var rankSearchTime;
        $('.rankSearch, .szRankSearch, .personRankSearch').live('focus', function () {
            var box = $(this);
            rankSearchImgUrl = box.css('background');
            box.css('background', 'none');
            rankSearchTime = setInterval(_rankSearchLoop(box), 1000);
        });
        $('.rankSearch, .szRankSearch, .personRankSearch').live('blur', function () {
            clearInterval(rankSearchTime);
            if ($(this).val() == '') {
                $(this).css('background', rankSearchImgUrl);
            }
        });
        //var searchTemp;
        $('#nameCodeInfos').live({
            mouseenter: function () {
                //clearTimeout(searchTemp);
                $('.rankSearch, .szRankSearch, .personRankSearch').unbind('blur');
            },
            mouseleave: function () {
                //clearTimeout(searchTemp);
                $('.rankSearch, .szRankSearch, .personRankSearch').blur(function () {
                    if ($('#nameCodeInfos') != undefined) {
                        $('#nameCodeInfos').hide();
                    }
                });
            }
        });
        var PageObject; //页面对象
        //系统级全局变量
        var marketRefreshTimer; //行情刷新全局定时器，在行情刷新页面使用

        //清理旧的内容
        //easyui应用部分元素会被加载到页面末尾，因为加载新内容时需要移除，防止大量生成代码堆积。
        function ClearBefore() {
            try {
                //销毁行情刷新计时器
                if (marketRefreshTimer != undefined) {
                    window.clearTimeout(marketRefreshTimer);
                }
                $('#mainContainer').nextAll().remove();
            } catch (e) {
            }
        }
        function _filter_staff_from_exist(_box) {
            return function () {
                filter_staff_from_exist(_box);
            }
        }
        var filter_staff_from_exist = function (box) {
            //var box = $('.nameCode');
            if ($.trim(box.val()).length &gt;= 1) {
                $.post('/Handlers/Default/CommonQueryHandler.ashx/?method=IndexCommodityList',
                {
                    nameCode: $.trim(box.val()),
                    market: (box.attr('market') == undefined) ? '' : box.attr('market')
                },
                function (data) {
                    //不存在DOM则新建
                    //if ($('#nameCodeInfos').length == 0) {
                        $('#nameCodeInfos').remove();
                        $(document.body).append('&lt;div id="nameCodeInfos" style="position: absolute; z-index: 999999999; text-align: left;background-color: #fff; border: 1px #ccc solid; display: none; _width:250px; overflow:hidden;"&gt;&lt;/div&gt;');
                        var infoLeft = box.offset().left;
                        var infoTop = box.offset().top + box.height();
                    //}
                    if (data.length &gt; 0) {
                        $('#nameCodeInfos').empty();
                        for (var i = 0; i &lt; data.length; i++) {
                            $('#nameCodeInfos').append('&lt;a&gt;' + data[i] + '&lt;/a&gt;');
                        }
                        //显示搜索面板
                        
                        //if ($('iframe', '#nameCodeInfos').length == 0) {
                        //    $('#nameCodeInfos').append('&lt;iframe style="_width:250px;_height:200px; top:0; left:0;position: absolute; z-index: -1; background-color:#fff;" frameborder="0"&gt;&lt;/iframe&gt;');
                        //}
                        //console.log(box.offset().left + ',' + box.offset().top);
                        $('#nameCodeInfos').offset({
                            left: infoLeft,
                            top: infoTop
                        });
                        $('#nameCodeInfos').show();
                        //触发文本框事件
                        $('.nameCode').unbind('blur');
                        $('.nameCode').blur(function () {
                            if ($('#nameCodeInfos') != undefined) {
                                $('#nameCodeInfos').hide();
                            }
                        });
                        $('#nameCodeInfos a').click(function () {
                            var tempVal = box.val();
                            if (box.val)
                                box.val($(this).text().split('/')[0]);
                            $('#nameCodeInfos').hide();
                            if (box.val() != tempVal) {
                              box.change();
                            }
                            //box.change();   //自动提示框选择前会导致下单的change事件触发，因此主动在填充后调用一次。
                        });
                    } else {
                        $('#nameCodeInfos').hide();
                    }
                },
                'json');
            } else {
                $('#nameCodeInfos').hide();
            }
        };
        var time;
        $('.nameCode').live('focus', function () {
            var box = $(this);
            box.css('background', 'none');
            time = setInterval(_filter_staff_from_exist(box), 1000);
        });
        $('.nameCode').live('blur', function () {
            clearInterval(time);
            if ($(this).val() == '') {
                $(this).css('background', 'url(/Skins/Default/images/searchInfotext.gif) no-repeat 5px 2px');
            }
        });
        var temp;
        $('#nameCodeInfos').live({
            mouseenter: function () {
                //clearTimeout(temp);
                //temp = setTimeout(function () {
                    $('.nameCode').unbind('blur');
                //}, 100);
            },
            mouseleave: function () {
                //clearTimeout(temp);
                //temp = setTimeout(function () {
                    $('.nameCode').blur(function () {
                        if ($('#nameCodeInfos') != undefined) {
                            $('#nameCodeInfos').hide();
                        }
                    });
                //}, 100);
            }
        });
        Number.prototype.toFixed = function (len) {
            return Math.round(this * Math.pow(10, len)) / Math.pow(10, len);
        }

        //行情图表窗口
        $('.lnkStockChart').live('click', function () {
            if ($('#stockChart').html() == null) {
                $(document.body).append('&lt;div id="stockChart"&gt;&lt;iframe id="ifrStockChart" frameborder="0" scrolling="no" src="/blank.htm" class="stock-ifr-size"&gt;&lt;/iframe&gt;&lt;/div&gt;');
                $('#stockChart').dialog({
                    title: '行情图表',
                    width: 760,
                    modal: true,
                    draggable: false,
                    cache: false,
                    minimizable: false,
                    maximizable: false,
                    collapsible: false,
                    closed: true,
                    onClose: function () {
                        $('#ifrStockChart').attr('src', 'javascript:;');  //防止闪烁
                    }
                });
            }
            $('#ifrStockChart').attr('src', '/Pages/Default/StockCharts/Frame.aspx?Code=' + $(this).attr('value'));
            $('#stockChart').dialog('open');
        });
