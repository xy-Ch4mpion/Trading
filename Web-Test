            var strhtml = '深圳国泰安教育技术股份有限公司 版权所有 &lt;span style="font-family: Arial;"&gt;&amp;copy;&lt;/span&gt; ShenZhen GTA Education Tech Ltd. All Right Reserved.';
            $.post('/Handlers/Default/CommonQueryHandler.ashx?method=GetGTACompanyName', function (data) {
                var chinesename = data.ChineseName;
                var englishname = data.EnglishName;
                strhtml = chinesename
                    + ' 版权所有 &lt;span style="font-family: Arial;"&gt;&amp;copy;&lt;/span&gt; '
