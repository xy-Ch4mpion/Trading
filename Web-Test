    <script type="text/javascript" language="javascript">
        $(function () {

        /*允许用户同时进入融资融券大赛和普通大赛
            //验证交易员是否进入的同一个大赛，防止用户同时进入多个大赛引起显示不正常的问题
            var localHref = window.location.href;
            //鼠标点击事件，检查COOKIE和交易员是否匹配
            var traderValid = function () {
                var matchTrader1 = localHref.match(/TraderID=[0-9]+/ig).toString().match(/[0-9]+/ig);
                var matchTrader2 = document.cookie.match(/TraderID=[0-9]+/ig).toString().match(/[0-9]+/ig);
                if (matchTrader1.toString() != '' &amp;&amp; matchTrader2.toString() != '') {
                    if (matchTrader1.toString() != matchTrader2.toString()) {
                        alert( '您已经登录其他大赛，请重新进入大赛才能进行操作。');
                        window.location.href = 'PreMatchIndex.aspx';
                        return;
                    }
                }
            }
            $('a,input').live('click', traderValid);
            $('a,input').live('mouseover', traderValid);
*/

            ///////////////////////////////////////////////////////////////////////////////

            //导航菜单样式初始化
            $('label.vslide-title', $('#nav-menu-box')).each(function () {
                $(this).next().hide();
            });
            //默认展开第一栏
            $('label.vslide-title:first', $('#nav-menu-box')).next().show();

            //栏目点击展开关闭
            $('label.vslide-title', $('#nav-menu-box')).click(function () {
                if ($(this).next().css('display') == 'none') {
                 $('label.vslide-title', $('#nav-menu-box')).filter(".exp").click();
                    $(this).removeClass('cls');
                    $(this).addClass('exp');
                    $(this).next().slideDown('fast');
                } else {
                    $(this).removeClass('exp');
                    $(this).addClass('cls');
                    $(this).next().slideUp('fast');
                }
            });

            //功能链接
            $('a', $('#nav-menu-box')).not($("#rzrqLink")).click(function () {
                if ($(this).attr('target') != '') {
                    return true;
                }
                $('a', $('#nav-menu-box')).removeClass('current');
                $(this).addClass('current');
                if ($(this).attr('href') != '#') {
                    PageObject = null; //清除页面对象
                    ClearBefore();  //清理Easyui生成的层代码
                    $('#frame').panel('refresh', $(this).attr('href'));
                }
                $('#link-current').text('&gt; ' + $(this).parent().prev().text() + ' &gt; ' + $(this).html());

                $(".compare").hide();
                return false;
            });
            $("#rzrqLink").click(function(){
             var currentTraderID=window.location.href.match(/TraderID=[0-9]+/ig).toString().match(/[0-9]+/ig);
             var url=$(this).attr("href")+"?TraderID="+currentTraderID;
             window.location.href=url;
             return false;
            });
            $('#frame').panel({
                cache: false,
                border: false
            });
            //比一比
            $("#btn-compare").click(function () {
                var herfStr = '/Pages/Default/Rankings/';
                var ids = "";
                var type = $(".compare-top").attr("type");
                var searchDate = $(".compare-top").attr("date");
                var compareCount = 0;
                $(".compare .man").each(function () {
                    if (ids == "") {
                        ids = $(this).attr("value");
                    } else {
                        ids += "," + $(this).attr("value");
                    }
                    compareCount += 1;
                });
                var winTitle;
                if (type == 0) {//用户比一比
                    if (compareCount &lt; 2) {
                        alert("请至少选择两个用户！");
                        return;
                    }
                    herfStr += "TraderCompare.aspx?TraderIDs=" + ids;
                    winTitle = "用户比一比";
                }
                else if (type == 1) {//赛组比一比
                    if (compareCount &lt; 2) {
                        alert("请至少选择两个赛组！");
                        return;
                    }
                    herfStr += "TeamCompare.aspx?TeamIDs=" + ids;
                    winTitle = "赛组比一比";
                } else {
                    alert("不能进行比较！");
                    return;
                }
                herfStr += "&amp;SearchDate=" + searchDate;
                if (!$("#divCompareWindow").html()) {
                    $("#mainbody").append('&lt;div id="divCompareWindow" style="_position:relative; _overflow-x:hidden;"&gt;&lt;/div&gt;');
                }
                $("#divCompareWindow").window({
                    href: herfStr,
                    title: winTitle,
                    height: 500,
                    width: 650,
                    shadow: true,
                    draggable: false,
                    modal: true,
                    cache: false,
                    minimizable: false,
                    maximizable: false,
                    collapsible: false
                });
            });
            $(".compare a").click(function () {
                $(".compare ul li").remove();
            })
            $(".compare").hide();

            //*****************************************
            //自动跳出提示框
            //*****************************************
            function CheckMessager() {
                $.post('/Handlers/Default/MyMessageHandler.ashx/?method=CheckMessager',
                {},
                function (data) {
                    var bonus = ["Bonus0", "Bonus1", "Bonus5", "Bonus6", "Bonus7", "Bonus8", "Bonus9", "Bonus13", "Bonus14", "Bonus15", "Bonus19", "Bonus20", "Bonus21", "Bonus22"];
                    var bonusNum = 0;
                    var allNum = 0;
                    var RZRQAdditionalNum = 0;
                    var shares = ["Shares16","Shares17","Shares18"];
                    var BondDividendNum=0;//债权派息提示信息数
                    var FundResolutionNum =0;//基金拆合股提示信息数
                    var SOExerciseNoticeNum =0;//到期行权提醒提示信息数
                    var ExercisetipsNum=0;//行权提示提示信息数                   
                    var sharesNum = 0;

//                    var closePositionNum = 0;//强平提示


                    for (var i = 0; i &lt; data.length; i++) {
                        var text = data[i].Content
                        if (/FoulsRecord\.aspx/ig.test(text)) {
                            text = text.replace(/&lt;a.+&lt;\/a&gt;/ig, '&lt;a href="javascript:void(0)" class="lnk" id="messager_' + data[i].ID + '"&gt;点击&lt;a/&gt;');
                        }
                        if (/InfomationNotice\.aspx/ig.test(text)) {
                            text = text.replace(/&lt;a.+&lt;\/a&gt;/ig, '&lt;a href="javascript:void(0)" class="lnk" id="infoNotice_' + data[i].ID + '"&gt;点击&lt;a/&gt;');
                        }
//                        if ($.inArray(data[i].Module, bonus) &lt; 0) {
//                            if (data[i].Module != 'RZRQAdditional') {
//                                $.messager.show({
//                                    title: data[i].Title,
//                                    msg: text,
//                                    timeout: 0,
//                                    showType: 'slide'
//                                });
//                            } else {
//                                RZRQAdditionalNum++;
//                            }
//                        } else {
//                            bonusNum++;
//                        }
                        if ($.inArray(data[i].Module, shares) &gt;= 0 || $.inArray(data[i].Module, bonus) &gt;= 0) {
                            $.messager.show({
                                    title: data[i].Title,
                                    msg: text,
                                    timeout: 0,
                                    showType: 'slide'
                                });
                             if($.inArray(data[i].Module, shares) &gt;= 0 ) {
                                sharesNum++;
                             } else {
                                allNum++;
                                switch(data[i].Module)
                                {
                                    case "Bonus19":
                                        BondDividendNum++;
                                        break;
                                    case "Bonus20":
                                        FundResolutionNum++;
                                        break;
                                          case "Bonus21":
                                        SOExerciseNoticeNum++;
                                        break;
                                          case "Bonus22":
                                        ExercisetipsNum++;
                                        break;
                                        default:
                                        bonusNum++;
                                        break;
                                }
                     
                             }
                        } else {
                             if (data[i].Module != 'RZRQAdditional') {
                                $.messager.show({
                                    title: data[i].Title,
                                    msg: text,
                                    timeout: 0,
                                    showType: 'slide'
                                });
                            } else {
                                RZRQAdditionalNum++;
                            }
                        }
                        $('#messager_' + data[i].ID).click(function () {
                            //触发违规记录导航点击
                            $('label[id="vslide-title-trade"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-weigui"]', $('#nav-menu-box')).click();
                        });
                        $('#infoNotice_' + data[i].ID).click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                    if (allNum &gt; 0) {  
                        var strMSG ="";
                        var tipHeight =80;                        
                          if(bonusNum&gt;0)
                          {
                            strMSG+= '你有' + bonusNum + '个分红信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }
                           if(BondDividendNum&gt;0)
                          {
                            strMSG+= '你有' + BondDividendNum + '个债权派息信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                              tipHeight+=30;
                          }
                           if(FundResolutionNum&gt;0)
                          {
                            strMSG+= '你有' + FundResolutionNum + '个基金拆合股信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }
                           if(SOExerciseNoticeNum&gt;0)
                          {
                            strMSG+= '你有' + SOExerciseNoticeNum + '个到期行权提醒信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }
                           if(ExercisetipsNum&gt;0)
                          {
                            strMSG+= '你有' + ExercisetipsNum + '个行权提示信息。&lt;a class="a_bonus" href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }                           
                        $.messager.show({
                            title: '提示信息',
                            msg: strMSG,
                            timeout: 0,
                            showType: 'slide',
                            height:tipHeight
                        });
                        $('.a_bonus').click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                    if (RZRQAdditionalNum &gt; 0) {
                        $.messager.show({
                            title: '融资融券追保',
                            msg: '你有' + RZRQAdditionalNum + '个融资融券追保信息。&lt;a id="a_RZRQAdditional" href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...',
                            timeout: 0,
                            showType: 'slide'
                        });
                        $('#a_RZRQAdditional').click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                    if(sharesNum &gt; 0)
                    {
                        $.messager.show({
                            title:'配股信息',
                            msg:'你有'+ sharesNum+'个配股提示信息。&lt;a id="a_shares" href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...',
                            timeout:0,
                            showType:'slide'
                        });
                         $('#a_shares').click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                },
                'json');
            }
            setTimeout(CheckMessager, 1000 * 3);    //第一次加载延迟3秒钟

                        
            /***********************************/
            //股票资讯搜索框
            var infomationBoxTimer = setTimeout(function(){
                $('#infomationBox').hide(500);
            },3000);    //第一次初始化后三秒后自动关闭资讯框，防止对内容进行长时间遮挡。
            $('#openInfomation').click(function(){
                window.clearTimeout(infomationBoxTimer);    //关闭计时器
                $('#infomationBox').toggle();
            }); //展示或打开资讯搜索框
            $('#btnGetInfomation').click(function(){
                window.open(marketInfomationUrl + $('#txtInfomationCode').val());
            }); //查询资讯信息
        });

      

        //是否显示比一比窗口
        function ShowCompareBox() {
            $(".compare ul li").remove();
            $(".compare").show();
        }
        //点击比较按钮处理
        function ClickCompareHandler(value, name, type, searchDate) {
            if ($(".compare-top").attr("type") &amp;&amp; $(".compare-top").attr("type") != type) {
                $(".compare ul li").remove();
            }
            var isExists = false;
            var total = 1;
            $(".compare .man").each(function () {
                if ($(this).attr("value") == value) {
                    $.messager.alert('提示信息', name + "已选择", 'info');
                    isExists = true;
                }
                else {
                    total += 1;
                }
            });
            if (total &gt; 3) {
                alert("最多只能有3个参加比较！");
                return;
            }
            if (!isExists) {
                if (name.length &gt; 14) {
                    $(".compare ul").append(" &lt;li class='man' title='点击删除" + name + "' value='" + value + "'&gt;" + name.substring(0, 14) + "...&lt;/li&gt;");
                } else {
                    $(".compare ul").append(" &lt;li class='man' title='点击删除" + name + "' value='" + value + "'&gt;" + name + "&lt;/li&gt;");
                }
            }

            $(".compare-top").attr("type", type).attr("date", searchDate);

            $(".compare ul li").click(function () {
                $(this).remove();
            });
        }

        //下单成功后回调事件
        function onDoOrderSuccess() {
            //关闭窗口
            $('#doOrderPanel').window('close', true);
        }

        //数据导出共用方法
        function DataExportFunc(url) {
            $("#export-iframe").attr("src", url);
        }
    </script>

