<script type="text/javascript" language="javascript">
    $(function () {
        //管理端超管端退出登录
        $('#logout').click(function () {
            $.post('/Handlers/Default/Common/AdminIdentityHandler.ashx/?method=Logout',
            {},
            function (data) {
                try {
                    if (data.Success) {
                        if (data.Type == '1') {
                            window.location.href = data.url;
                        } else {
                            if (data.Type == '0') {
                                window.location.href = '/Mi/AdminLogin.aspx';
                            } else {
                                window.location.href = data.url;
                            }
                        }
                    }
                } catch (e) {
                    window.location.href = window.location.href;
                }
            },
            'json');
        });
        //竞赛端返回首页
        $('#returnHomePage').click(function () {
            $.post('/Handlers/HomePagesHandler.ashx/?method=GetVisitHomeUrl',
            {},
            function (data) {
                window.location.href = data.url;
            },
            'json');
        });


        /*           每30秒获取一次注册人数、在线人数、当前时间信息 begin    */
        var initdata = function () {
            $.post('/Handlers/Default/Common/IdentityHandler.ashx/?method=GetInitData', {}, function (data) {
                if (data) {
                    $("#lblUserNum").html("注册人数：" + data.UserNum);
                    $("#lblUserOnline").html("在线人数：" + data.UserOnline);
                    $("#lblNowDate").html(data.NowDate);
                    $("#lblNowTime").html(data.NowTime);
                }
            }, "json");
        };
        initdata();
        setInterval(initdata, 60000);
        /*           每60秒获取一次注册人数、在线人数、当前时间信息 end    */

        var customerlogoUrl = '/Skins/Default/images/logo.png';
        $('#header div.site').css("background", "url('" + customerlogoUrl + "') no-repeat scroll 0px 0px/220px 55px rgba(0, 0, 0, 0)");
        $('#iquantLoginimg').click(function () {
            //            window.location.href = window.location.href;
            if ("/index.aspx" == "/Index.aspx") { 
                return;
            }
            window.location.href = '/index.aspx';
        });
