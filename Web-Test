            //自动跳出提示框
            //*****************************************
            function CheckMessager() {
                $.post('/Handlers/Default/MyMessageHandler.ashx/?method=CheckMessager',
                {},
                function (data) {
                    var bonus = ["Bonus0", "Bonus1", "Bonus5", "Bonus6", "Bonus7", "Bonus8", "Bonus9", "Bonus13", "Bonus14", "Bonus15", "Bonus19", "Bonus20", "Bonus21", "Bonus22"];
                    var bonusNum = 0;
                    var allNum = 0;
                    var RZRQAdditionalNum = 0;
                    var shares = ["Shares16","Shares17","Shares18"];
                    var BondDividendNum=0;//债权派息提示信息数
                    var FundResolutionNum =0;//基金拆合股提示信息数
                    var SOExerciseNoticeNum =0;//到期行权提醒提示信息数
                    var ExercisetipsNum=0;//行权提示提示信息数                   
                    var sharesNum = 0;

//                    var closePositionNum = 0;//强平提示


                    for (var i = 0; i &lt; data.length; i++) {
                        var text = data[i].Content
                        if (/FoulsRecord\.aspx/ig.test(text)) {
                            text = text.replace(/&lt;a.+&lt;\/a&gt;/ig, '&lt;a href="javascript:void(0)" class="lnk" id="messager_' + data[i].ID + '"&gt;点击&lt;a/&gt;');
                        }
                        if (/InfomationNotice\.aspx/ig.test(text)) {
                            text = text.replace(/&lt;a.+&lt;\/a&gt;/ig, '&lt;a href="javascript:void(0)" class="lnk" id="infoNotice_' + data[i].ID + '"&gt;点击&lt;a/&gt;');
                        }
//                        if ($.inArray(data[i].Module, bonus) &lt; 0) {
//                            if (data[i].Module != 'RZRQAdditional') {
//                                $.messager.show({
//                                    title: data[i].Title,
//                                    msg: text,
//                                    timeout: 0,
//                                    showType: 'slide'
//                                });
//                            } else {
//                                RZRQAdditionalNum++;
//                            }
//                        } else {
//                            bonusNum++;
//                        }
                        if ($.inArray(data[i].Module, shares) &gt;= 0 || $.inArray(data[i].Module, bonus) &gt;= 0) {
                            $.messager.show({
                                    title: data[i].Title,
                                    msg: text,
                                    timeout: 0,
                                    showType: 'slide'
                                });
                             if($.inArray(data[i].Module, shares) &gt;= 0 ) {
                                sharesNum++;
                             } else {
                                allNum++;
                                switch(data[i].Module)
                                {
                                    case "Bonus19":
                                        BondDividendNum++;
                                        break;
                                    case "Bonus20":
                                        FundResolutionNum++;
                                        break;
                                          case "Bonus21":
                                        SOExerciseNoticeNum++;
                                        break;
                                          case "Bonus22":
                                        ExercisetipsNum++;
                                        break;
                                        default:
                                        bonusNum++;
                                        break;
                                }
                     
                             }
                        } else {
                             if (data[i].Module != 'RZRQAdditional') {
                                $.messager.show({
                                    title: data[i].Title,
                                    msg: text,
                                    timeout: 0,
                                    showType: 'slide'
                                });
                            } else {
                                RZRQAdditionalNum++;
                            }
                        }
                        $('#messager_' + data[i].ID).click(function () {
                            //触发违规记录导航点击
                            $('label[id="vslide-title-trade"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-weigui"]', $('#nav-menu-box')).click();
                        });
                        $('#infoNotice_' + data[i].ID).click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                    if (allNum &gt; 0) {  
                        var strMSG ="";
                        var tipHeight =80;                        
                          if(bonusNum&gt;0)
                          {
                            strMSG+= '你有' + bonusNum + '个分红信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }
                           if(BondDividendNum&gt;0)
                          {
                            strMSG+= '你有' + BondDividendNum + '个债权派息信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                              tipHeight+=30;
                          }
                           if(FundResolutionNum&gt;0)
                          {
                            strMSG+= '你有' + FundResolutionNum + '个基金拆合股信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }
                           if(SOExerciseNoticeNum&gt;0)
                          {
                            strMSG+= '你有' + SOExerciseNoticeNum + '个到期行权提醒信息。&lt;a class="a_bonus"  href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }
                           if(ExercisetipsNum&gt;0)
                          {
                            strMSG+= '你有' + ExercisetipsNum + '个行权提示信息。&lt;a class="a_bonus" href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...&lt;p/&gt;'
                            tipHeight+=30;
                          }                           
                        $.messager.show({
                            title: '提示信息',
                            msg: strMSG,
                            timeout: 0,
                            showType: 'slide',
                            height:tipHeight
                        });
                        $('.a_bonus').click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                    if (RZRQAdditionalNum &gt; 0) {
                        $.messager.show({
                            title: '融资融券追保',
                            msg: '你有' + RZRQAdditionalNum + '个融资融券追保信息。&lt;a id="a_RZRQAdditional" href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...',
                            timeout: 0,
                            showType: 'slide'
                        });
                        $('#a_RZRQAdditional').click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                    if(sharesNum &gt; 0)
                    {
                        $.messager.show({
                            title:'配股信息',
                            msg:'你有'+ sharesNum+'个配股提示信息。&lt;a id="a_shares" href="javascript:void(0)" class="lnk"&gt;点击&lt;/a&gt; 查看信息列表...',
                            timeout:0,
                            showType:'slide'
                        });
                         $('#a_shares').click(function () {
                            //触发信息提示导航点击
                            $('label[id="vslide-title-infoCenter"].cls', $('#nav-menu-box')).click();
                            $('a[id="lnk-to-infoNotice"]', $('#nav-menu-box')).click();
                        });
                    }
                },
                'json');
            }
            setTimeout(CheckMessager, 1000 * 3);    //第一次加载延迟3秒钟

                        
            /***********************************/
            //股票资讯搜索框
            var infomationBoxTimer = setTimeout(function(){
                $('#infomationBox').hide(500);
            },3000);    //第一次初始化后三秒后自动关闭资讯框，防止对内容进行长时间遮挡。
            $('#openInfomation').click(function(){
                window.clearTimeout(infomationBoxTimer);    //关闭计时器
                $('#infomationBox').toggle();
            }); //展示或打开资讯搜索框
            $('#btnGetInfomation').click(function(){
                window.open(marketInfomationUrl + $('#txtInfomationCode').val());
            }); //查询资讯信息
        });

      

        //是否显示比一比窗口
        function ShowCompareBox() {
            $(".compare ul li").remove();
            $(".compare").show();
        }
        //点击比较按钮处理
        function ClickCompareHandler(value, name, type, searchDate) {
            if ($(".compare-top").attr("type") &amp;&amp; $(".compare-top").attr("type") != type) {
                $(".compare ul li").remove();
            }
            var isExists = false;
            var total = 1;
            $(".compare .man").each(function () {
                if ($(this).attr("value") == value) {
                    $.messager.alert('提示信息', name + "已选择", 'info');
                    isExists = true;
                }
                else {
                    total += 1;
                }
            });
            if (total &gt; 3) {
                alert("最多只能有3个参加比较！");
                return;
            }
            if (!isExists) {
                if (name.length &gt; 14) {
                    $(".compare ul").append(" &lt;li class='man' title='点击删除" + name + "' value='" + value + "'&gt;" + name.substring(0, 14) + "...&lt;/li&gt;");
                } else {
                    $(".compare ul").append(" &lt;li class='man' title='点击删除" + name + "' value='" + value + "'&gt;" + name + "&lt;/li&gt;");
                }
            }

            $(".compare-top").attr("type", type).attr("date", searchDate);

            $(".compare ul li").click(function () {
                $(this).remove();
            });
        }

        //下单成功后回调事件
        function onDoOrderSuccess() {
            //关闭窗口
            $('#doOrderPanel').window('close', true);
        }

        //数据导出共用方法
        function DataExportFunc(url) {
            $("#export-iframe").attr("src", url);
        }
    </script>

